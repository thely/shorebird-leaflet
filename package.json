{
  "name": "shorebirds-leaflet",
  "version": "1.0.0",
  "description": "leaflet version of shorebirds",
  "main": "bundle.js",
  "scripts": {
    "build": "browserify src/js/main.js -o assets/js/bundle.js",
    "watch": "watchify src/js/main.js -o assets/js/bundle.js --debug --verbose -t [ babelify --presets [@babel/preset-env] ]",
    "scss": "node-sass --watch src/scss/style.scss assets/css/style.css",
    "start": "serve",
    "dev": "npm-run-all --parallel start scss watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "audiobuffer-loader": "^1.0.3",
    "binauralfir": "^0.1.2",
    "chance": "^1.0.18",
    "leaflet": "^1.5.1",
    "leaflet-sidebar-v2": "^3.2.0",
    "sample-manager": "^2.1.4",
    "standardized-audio-context": "^20.1.12",
    "tablesort": "^5.1.0",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "archiver": "^3.0.3",
    "babelify": "^10.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "serve": "^11.1.0"
  }
}
